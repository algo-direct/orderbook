#include <iostream>
#include <map>
#include <string>

void prim(const std::map<int, std::string>& m) {
    for(auto& e: m) {
        std::cout << e.first << " : " << e.second << std::endl;
    }
}

void prii(auto& it, auto& cont) {
    if(cont.end() == it) {
        std::cout << " end" <<  std::endl;
        return;
    }
    std::cout << it->first << " : " << it->second << std::endl;
}

int main() {
    std::map<int, std::string> student_grades;
    
    // Insert some elements into the map
    student_grades[101] = "Alice";
    student_grades[105] = "Bob";
    student_grades[110] = "Charlie";
    student_grades[115] = "David";
    student_grades[120] = "Eve";
    prim(student_grades);

    auto  it = std::lower_bound(student_grades.begin(), student_grades.end(), 101);
    prii(it, student_grades);

    return 0;
}